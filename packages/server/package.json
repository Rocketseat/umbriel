{
  "name": "@umbriel/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "dev:queue": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/queue/runner.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && copyfiles -u 1 src/**/*.html dist",
    "start:server": "node dist/shared/infra/http/server.js",
    "start:queue": "node dist/shared/infra/queue/runner.js",
    "test": "jest --runInBand",
    "lint": "eslint src --ext ts"
  },
  "engines": {
    "node": ">=12.3.1"
  },
  "dependencies": {
    "@sentry/node": "5.11.2",
    "aws-sdk": "^2.608.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.14.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "html-to-text": "^5.1.1",
    "js-beautify": "^1.10.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.0.1",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.13.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/html-to-text": "^1.4.31",
    "@types/ioredis": "^4.14.4",
    "@types/jest": "^24.9.0",
    "@types/js-beautify": "^1.8.1",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mongoose": "^5.5.41",
    "@types/multer": "^1.3.10",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "copyfiles": "^2.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "ioredis": "^4.14.1",
    "jest": "^24.9.0",
    "mongodb": "^3.5.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^3.7.5"
  }
}
