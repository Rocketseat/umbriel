{
  "name": "@umbriel/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "start": "npm-run-all -p start:*",
    "dev:server": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/http/server.ts",
    "dev:queue": "ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/queue/runner.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && copyfiles -u 1 src/**/*.html dist",
    "start:server": "node dist/shared/infra/http/server.js",
    "start:queue": "node dist/shared/infra/queue/runner.js",
    "lint": "eslint src --ext ts",
    "tsc": "tsc --noEmit"
  },
  "engines": {
    "node": ">=12.3.1"
  },
  "dependencies": {
    "@sentry/node": "6.3.1",
    "aws-sdk": "2.892.0",
    "bcryptjs": "2.4.3",
    "bull": "3.22.3",
    "cors": "2.8.5",
    "csv-parse": "4.15.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-async-errors": "3.1.1",
    "html-to-text": "7.1.1",
    "ioredis": "4.27.1",
    "js-beautify": "1.13.13",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.12.5",
    "multer": "1.4.2",
    "nodemailer": "6.5.0",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.5.0",
    "winston": "3.3.3",
    "winston-mongodb": "5.0.7",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "1.2.4",
    "@types/bcryptjs": "2.4.2",
    "@types/bull": "3.15.1",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/html-to-text": "6.0.0",
    "@types/ioredis": "4.26.0",
    "@types/jest": "^26.0.22",
    "@types/js-beautify": "1.13.1",
    "@types/jsonwebtoken": "8.5.1",
    "@types/mocha": "^8.2.2",
    "@types/mongoose": "5.10.5",
    "@types/multer": "1.4.5",
    "@types/nodemailer": "6.4.1",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "copyfiles": "2.4.1",
    "eslint": "7.25.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "mongodb": "3.6.6",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "4.2.4"
  }
}
